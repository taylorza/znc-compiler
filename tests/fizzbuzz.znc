int printnum(int n) {
  int hasvalue = 0;
  int div = 10000;

  while((n / div) == 0) {
    div = div / 10;
  }
  
  while(div) {
    putc(n / div + '0');
    n = n % div;			
	div = div / 10;
  }
}

