void memcpy(void *dst, void *src, int len) __asm__ {
  ld ix,0
  add ix,sp
  
  ld c,(ix+2)
  ld b,(ix+3)

  ld l,(ix+4)
  ld h,(ix+5)

  ld e,(ix+6)
  ld d,(ix+7)

  ldir
      
  ret
}

int printnum(int n) {
  int div = 10000;

  while((n / div) == 0) {
    div = div / 10;
  }
  
  while(div) {
    putc(n / div + '0');
    n = n % div;			
	  div = div / 10;
  }
}
